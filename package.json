{
  "name": "static-starter",
  "version": "0.1.0",
  "description": "",
  "dependencies": {
    "vh-check": "^2.0.5"
  },
  "private": true,
  "repository": {
    "url": "https://github.com/jmegs/static-starter"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@fec/eleventy-plugin-remark": "^1.0.2",
    "css-loader": "^3.5.3",
    "luxon": "^1.23.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "remark": "^12.0.0",
    "remark-html": "^11.0.1",
    "remark-unwrap-images": "^2.0.0",
    "require-dir": "^1.2.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpackbar": "^4.0.0"
  },
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf _site/*",
    "build": "ELEVENTY_ENV=production run-s clean build:*",
    "build:site": "eleventy --config=eleventy.config.js",
    "build:assets": "webpack -p",
    "postbuild": "prettier --write '_site/**/*.{html,css}'",
    "watch": "ELEVENTY_ENV=development run-s clean && run-p watch:*",
    "watch:site": "eleventy --serve --port=3000 --config=eleventy.config.js --quiet",
    "watch:assets": "webpack -d --watch"
  },
  "author": "John Meguerian <johnmeguerian@gmail.com>",
  "license": "ISC"
}
